<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="us-pathology-message-header"/>
    <meta>
        <versionId value="10"/>
        <lastUpdated value="2022-06-23T19:36:15.268+00:00"/>
